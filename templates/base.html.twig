        <div id="navbar" class="collapse navbar-collapse">
            <ul class="nav navbar-nav">
                <li class="active"><a href="{{ path('homepage') }}">Homepage</a></li>
                {#                <li><a href="{{ path('todo_create') }}">Add Todo</a></li>#}
